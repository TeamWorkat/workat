<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-config.dtd">
<mapper namespace="org.workat.workat_project.place.repository.PlaceMapper">
	<!-- Place 테이블 가져오기 -->
    <select id="getMainViewPlaceList" resultType="org.workat.workat_project.place.entity.PlaceListDTO">
    	SELECT p.place_id, p.place_nm, pp.picture_source  FROM place p, place_picture pp where p.place_id = pp.place_id ;
    </select>
</mapper>