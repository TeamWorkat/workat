<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.workat.workat_project.user.repository.UserMapper">

    <select id="getUserList" resultType="UserVO">
        select * from user
    </select>

    <select id="getUserDetail" resultType="org.workat.workat_project.user.entity.UserDetailDTO">
        select *
        from user
        where user_id = #{user_id}
    </select>

</mapper>